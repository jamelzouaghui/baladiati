

{% extends 'template.html.twig' %}

{% block body %}

    <div id= "ContactForm" class="box-body">
        {{form_start(form)}}

        <div class="form-group">
            <div class="form-group">
                {{ form_row(form.title)}}
            </div>
            <div class="form-group">
                {{ form_row(form.createdAt)}}
            </div>
            <div class="form-group">
                {{ form_row(form.adress)}}
            </div>
        </div>

    </div>


    <input type="submit" class="btn  btn-success" value="Enregister"/>
    {{form_end(form)}}
</div>
{% endblock %}